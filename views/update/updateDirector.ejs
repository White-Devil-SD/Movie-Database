                <div class="col-md-8">
                    <h3 class="mb-5"> Update Director Details By Name:</h3>

                </div>

                <form action="/update1" method="post">

                    <div class="form-group row">
                        <label for="example-search-input" class="col-2 label col-form-label">Enter Name:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="name" type="text" maxlength="" placeholder="Name" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="example-search-input" class="col-2 label col-form-label">Phone:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="phone" type="text" maxlength="10" placeholder=" new phone no" required>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="example-text-input" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control  btn-outline-success " name="" type="submit" value="submit">
                        </div>
                    </div>
                </form>
            </div>

            <!-- <%- include ../../partials/sidebar %> -->

        </div>


    </div>

    </div>



    <div class="container">


        <div class="row">

            <div class="col-md-10   ">
                <div class="col-md-8">
                    <h3 class="mb-5"> Update Movie Details By Title:</h3>

                </div>

                <form action="/update2" method="post">


                    <div class="form-group row">
                        <label for="example-search-input" class="col-2 label col-form-label">Title:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="title" type="text" placeholder="movieName" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="example-search-input" class="col-2 label col-form-label">Mov language:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="lang" type="text" placeholder="language" required>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="example-text-input" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control  btn-outline-success " name="" type="submit" value="submit">
                        </div>
                    </div>


                </form>

            </div>
