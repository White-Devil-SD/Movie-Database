<body>
    <header>
        <%- include ../partials/navbar %>


            <div class="container" id="target">
                <h1 class="display-3 text-center">Insert Here</h1>
            </div>
    </header>

    <div class="container">


        <div class="row">

            <div class="col-md-8   ">
                <div class="col-md-8">
                    <h3 class="mb-5">Rating Details:</h3>

                </div>
                <form action="/insert/ratings" method="post">
                    <div class="form-group row">
                        <label for="example-text-input" class="col-2 label col-form-label">Movie Id:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="mov_id" maxlength="4" type="text" placeholder="0000" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="example-search-input" class="col-2 label col-form-label">Ratings:</label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control" name="rating" type="text" maxlength="2" placeholder="8" required>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="example-text-input" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input id="inputClass" class="form-control  btn-outline-success " name="" type="submit" value="submit">
                        </div>
                    </div>

                </form>

            </div>
            <%- include ../partials/sidebar %>

        </div>

    </div>


    <script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n"
